<apex:page standardController="Quote" extensions="SaveQuoteAsPdf" renderAs="pdf" showHeader="false" action="{!CreateQuote}" standardStylesheets="false" >


<head>
        <title>{!ObjAccount.name} R{!ObjQuote.QU_REVISION_NUMBER__c}</title>
        <style type="text/css">
