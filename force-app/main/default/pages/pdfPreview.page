                                                    <td style="text-align: left; vertical-align:top;">
                                                        <apex:outputPanel rendered="{!QLOpt.Pdf_Checked}">
                                                            <span>{!IF(QLOpt.Quantity != null, QLOpt.Quantity, '')}</span>
                                                        </apex:outputPanel>
                                                    </td>
                                                    <td style="text-align: right; vertical-align:top;">
